[web]
# host can be ip
# 52.68.101.18

# or name with host ip attached as a variable
my_host_1 ansible_ssh_host=52.193.213.21
my_host_2 ansible_ssh_host=52.68.101.18

# variables can be set in the same file
# custom variables can be set within group variables
# or in group_vars directory
# # [web:vars]
# # ansible_user=ubuntu 
# # ansible_ssh_private_key_file=~/.ssh/ec2.pem
# # my_variable=foo
